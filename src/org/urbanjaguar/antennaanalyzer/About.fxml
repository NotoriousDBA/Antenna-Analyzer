<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.urbanjaguar.antennaanalyzer.AboutController">
   <children>
      <TextArea fx:id="aboutText" editable="false" prefHeight="400.0" prefWidth="600.0">
         <VBox.margin>
            <Insets left="14.0" right="14.0" top="14.0" />
         </VBox.margin>
      </TextArea>
      <Button fx:id="btnClose" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onMouseClicked="#CloseClick" text="Close">
         <VBox.margin>
            <Insets bottom="14.0" left="14.0" right="14.0" top="14.0" />
         </VBox.margin>
      </Button>
   </children>
</VBox>
